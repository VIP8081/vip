import os
import subprocess
#os.system('git pull')
def download_and_run(url, filename):
    if not os.path.isfile(filename):
        os.system(f'curl -L {url} > {filename}')
        os.system(f'chmod 777 {filename}')
    
    os.system(f'./{filename}')

